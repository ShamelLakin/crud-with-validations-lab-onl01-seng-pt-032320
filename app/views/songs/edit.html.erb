<%= form_for @song do |f| %>
    <%= f.label :title %>
    <%= f.text_field :title %><br />
    <%= f.label :released %>
    <%= f.text_field :released %>
    <%= f.label :released_year %><br />
    <%= f.text_field :released_year %>
    <%= f.label :artist_name %><br />
    <%= f.text_field :artist_name %>
    <%= f.label :genre %><br />
    <%= f.text_field :genre %>
    <br />
    <%= f.submit %>
    <% end %>